</nav>

<% @matchs.each do |match|%>
<div class="card m-2 text-center">
<%=link_to match.created_at.in_time_zone("Tokyo").strftime("%Y年%m月%d日"), match_path(match) %>
<%= button_to "Delete", match_path(match), class: "btn btn-outline-danger btn-sm", method: :delete %>
</div>
<% end %>
